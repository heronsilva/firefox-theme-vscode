{
  "$schema": "vscode://schemas/color-theme",
  "name": "Firefox Dark",
  "type": "dark",
  "author": "Heron Silva",
  "semanticClass": "theme.firefox-dark",
  "semanticHighlighting": false,

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#939393"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#6B89FF"
      }
    },
    {
      "name": "Template string",
      "scope": ["string.template"],
      "settings": {
        "foreground": "#B1B1B3"
      }
    },
    {
      "name": "Regexp",
      "scope": [
        "constant.character.escape.backslash.regexp",
        "constant.other.character-class.regexp",
        "keyword.operator.quantifier.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#6B89FF"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#6B89FF"
      }
    },
    {
      "name": "Variable",
      "scope": ["meta.definition.variable.name", "support.variable", "variable", "variable.language"],
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FE7DE9"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#FE7DE9"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#B98EFF"
      }
    },
    {
      "name": "Function name",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#B98EFF"
      }
    },
    {
      "name": "Function definition",
      "scope": ["meta.definition.function"],
      "settings": {
        "foreground": "#75BFFF"
      }
    },

    {
      "scope": ["meta.brace.round"],
      "settings": {
        "foreground": "#B1B1B3"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": ["punctuation.section.embedded.begin", "punctuation.section.embedded.end"],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#FFFFB4"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#75BFFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#FE7DE9"
      }
    },
    {
      "name": "Function",
      "scope": "support.function",
      "settings": {
        // "foreground": "#B98EFF"
        "foreground": "#DF80FF"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#6B89FF"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#B98EFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.function",
        // "support.variable.property.dom",
        // "variable.other.property",
        "entity.name.variable.field",
        "support.function.any-method",
        "support.function.lua"
      ],
      "settings": {
        "foreground": "#87DE74"
      }
    },
    {
      "scope": ["meta.object-literal.key" /*, "variable.other.readwrite" */],
      "settings": {
        // "foreground": "#87DE74"
        "foreground": "#75BFFF"
      }
    },
    {
      "name": "dots, brackets, commas, etc.",
      "scope": [
        "keyword.operator.assignment",
        "meta.brace.square",
        "punctuation.accessor",
        "punctuation.bracket",
        "punctuation.curlybrace",
        "punctuation.definition.block",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.typeparameters",
        "punctuation.parenthesis",
        "punctuation.section.braces",
        "punctuation.section.class",
        "punctuation.section.method",
        "punctuation.section.parens",
        "punctuation.separator.comma",
        "punctuation.separator.delimiter",
        "punctuation.separator.key-value",
        "punctuation.terminator.statement",
        "punctuation.terminator",
        "keyword.operator.arithmetic",
        "punctuation.separator.parameter"
      ],
      "settings": {
        "foreground": "#909090"
      }
    },
    {
      "scope": "storage.type.function.arrow.js",
      "settings": {
        "foreground": "#B98EFF"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        // "foreground": "#FE7DE9",
        "foreground": "#DF80FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type.class", "entity.name.type.interface"],
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "scope": ["entity.name.type.interface"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    // {
    //   "scope": "keyword.operator.assignment",
    //   "settings": {
    //     "foreground": "#FBFBFE"
    //   }
    // },
    {
      "scope": ["source.env", "constant.numeric.env"],
      "settings": {
        "foreground": "#B1B1B3"
      }
    },
    {
      // "scope": ["storage.modifier.const"],
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#B98EFF"
      }
    },

    {
      "scope": ["variable.other.object", "variable.parameter"],
      "settings": {
        // "foreground": "#B98EFF",
        "foreground": "#75BFFF"
      }
    },
    // {
    //   "name": "Function argument",
    //   "scope": ["variable.parameter"],
    //   "settings": {
    //     "foreground": "#75BFFF",
    //     "fontStyle": "italic"
    //   }
    // },
    {
      "scope": "variable.other.constant.property",
      "settings": {
        "foreground": "#87DE74"
      }
    },
    {
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#909090"
      }
    },
    {
      "scope": ["constant.language.boolean.true"],
      "settings": {
        "foreground": "#87DE74"
      }
    },
    {
      "scope": ["constant.language.boolean.false"],
      "settings": {
        "foreground": "#FF9494"
      }
    },

    {
      "scope": ["source.lua"],
      "settings": {
        "foreground": "#B1B1B3"
      }
    },

    // #region HTML
    {
      "scope": "text.html.derivative",
      "settings": {
        "foreground": "#D7D7DB"
        // "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "name": "DOCTYPE",
      "scope": ["meta.tag.metadata.doctype.html", "meta.tag.sgml.doctype.html"],
      "settings": {
        "foreground": "#909090"
      }
    },
    {
      "name": "Tag start/end",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#767578"
      }
    },
    {
      "name": "Unnamed",
      "scope": "meta.tag.structure.any.html",
      "settings": {
        "foreground": "#B98EFF"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute.name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.id.html",
        "entity.other.attribute-name.localname.xml"
        // "entity.other.attribute-name.js"
      ],
      "settings": {
        "foreground": "#FE7DE9"
      }
    },
    {
      "name": "HTML Comment",
      "scope": "comment.block.html",
      "settings": {
        "foreground": "#87DE74AA"
      }
    },
    {
      "name": "HTML String begin/end",
      "scope": ["punctuation.definition.string.begin.html", "punctuation.definition.string.end.html"],
      "settings": {
        "foreground": "#909090"
      }
    },
    {
      "scope": ["string.quoted.double.html", "string.quoted.double.xml" /* , "meta.attribute.name" */],
      "settings": {
        "foreground": "#B98EFF"
      }
    },
    // #endregion HTML

    // #region CSS
    {
      "scope": ["meta.selector.css", "entity.name.tag.css", "keyword.control.at-rule.media.css"],
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "scope": [
        "meta.function.url.css",
        "meta.property-value.css",
        "support.constant.property-value.css",
        "support.function.url.css",
        "support.function.misc.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.pt.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.ch.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.scss",
        "keyword.other.unit.dppx.css",
        "support.constant.media.css",
        "constant.numeric.css",
        "string.quoted.single.css",
        "string.quoted.double.css"
      ],
      "settings": {
        "foreground": "#6B89FF"
      }
    },
    {
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "variable.parameter.url.css",
        "support.function.transform.css",
        "support.function.calc.css",
        "entity.other.attribute-name.pseudo-class.css",
        "meta.function.gradient.css",
        "support.function.gradient.css",
        "variable.parameter.url.scss",
        "entity.name.tag.scss"
      ],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "scope": [
        "punctuation.terminator.rule.css",
        "punctuation.separator.key-value.css",
        "punctuation.section.property-list.begin.bracket.curly.css",
        "punctuation.section.property-list.end.bracket.curly.css",
        "punctuation.section.function.begin.bracket.round.css",
        "punctuation.section.function.end.bracket.round.css",
        "punctuation.definition.entity.begin.bracket.square.css",
        "punctuation.definition.entity.end.bracket.square.css",
        "punctuation.separator.list.comma.css",
        "keyword.operator.pattern.css"
      ],
      "settings": {
        "foreground": "#D7D7DB"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.media.css",
        "support.type.vendored.property-name.media.css"
      ],
      "settings": {
        "foreground": "#87DE74"
      }
    },
    {
      "scope": ["support.type.vendored.property-name.css", "entity.other.attribute-name.pseudo-element.css"],
      "settings": {
        "foreground": "#939393"
      }
    },
    {
      "scope": "keyword.operator.combinator.css",
      "settings": {
        "foreground": "#D7D7DB"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#87DE74"
      }
    },
    // #endregion

    // #region JSON
    {
      "name": "JSON Property",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "name": "JSON String",
      "scope": ["string.quoted.double.json"],
      "settings": {
        // "foreground": "#6B89FF"
        "foreground": "#FF7DE9"
      }
    },
    {
      "name": "JSON Numeric",
      "scope": ["constant.numeric.json", "constant.language.json"],
      "settings": {
        "foreground": "#87DE74"
      }
    },
    {
      "scope": ["punctuation.separator.dictionary.key-value.json", "punctuation.separator.dictionary.pair.json"],
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "scope": ["punctuation.separator.dictionary.key-value.json", "punctuation.separator.dictionary.pair.json"],
      "settings": {
        "foreground": "#B1B1B3"
      }
    },
    // #endregion JSON

    // #region MARKDOWN
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#87DE74",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#75BFFF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#87DE74"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#CC3D3D"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#0A84FF"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#B98EFF"
      }
    },
    {
      "scope": ["markup.underline.link.markdown", "markup.underline.link.image.markdown"],
      "settings": {
        "foreground": "#75BFFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#FE7DE9"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#75BFFF"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#B1B1B3"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#B1B1B3"
      }
    },
    {
      "scope": ["string.other.link.title.markdown", "string.other.link.description.markdown"],
      "settings": {
        "foreground": "#E9F4FE",
        "fontStyle": "underline"
      }
    }
    // #endregion MARKDOWN
  ],

  "colors": {
    // Base Colors
    "foreground": "#B1B1B3",
    "focusBorder": "#0A84FF",
    "contrastBorder": "#3C3C3D",
    "widget.shadow": "#00000000",
    "selection.background": "#204E8A",
    "descriptionForeground": "#FFFFFF",
    "errorForeground": "#CC3D3D",

    // Text Colors
    "textBlockQuote.background": "#353B48",
    "textBlockQuote.border": "#0A84FF",
    "textCodeBlock.background": "#353B48",
    "textLink.activeForeground": "#75BFFF",
    "textLink.foreground": "#75BFFF",
    "textPreformat.foreground": "#D7BA7D",
    "textSeparator.foreground": "#3C3C3D",

    // Button Colors
    "button.background": "#252526",
    "button.foreground": "#B1B1B3",
    "button.hoverBackground": "#353B48",

    // Dropdown Colors
    "dropdown.background": "#4A4A4F",
    "dropdown.listBackground": "#5C5C61",
    "dropdown.border": "#3C3C3D",
    "dropdown.foreground": "#F9F9FA",

    // Input Colors
    "input.background": "#141416",
    "input.border": "#3C3C3D",
    "input.foreground": "#FFFFFF",
    "input.placeholderForeground": "#B1B1B3",
    "inputOption.activeBorder": "#0A84FF",
    "inputValidation.errorBackground": "#4B3137",
    "inputValidation.errorForeground": "#FF9494",
    "inputValidation.errorBorder": "#4B3137",
    "inputValidation.infoBackground": "#204E8A",
    "inputValidation.infoForeground": "#FFFFFF",
    "inputValidation.infoBorder": "#204E8A",
    "inputValidation.warningBackground": "#42381F",
